{
  "name": "json schema fluent builder",
  "version": "0.1.0",
  "description": "A package to build json schemas.",
  "repository": {
    "type": "git",
    "url": "https://github.com/DynasoftBr/json-schema-fluent-builder"
  },
  "author": "Leandro Albano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DynasoftBr/json-schema-fluent-builder/issues"
  },
  "homepage": "https://github.com/DynasoftBr/json-schema-fluent-builder",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"nodemon dist/server.js\"",
    "test": "mocha dist/test",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "devDependencies": {
    "@types/node": "^7.0.22",
    "@types/mocha": "2.2.41",
    "@types/chai": "4.0.0",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2",
    "mocha": "3.4.2",
    "chai": "4.0.1"
  }
}